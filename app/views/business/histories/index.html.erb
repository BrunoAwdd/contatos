<p id="notice"><%= notice %></p>

<h1>Business Histories</h1>

<table>
  <thead>
    <tr>
      <th>Data entry</th>
      <th>Note</th>
      <th>Business general</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_histories.each do |business_history| %>
      <tr>
        <td><%= business_history.data_entry %></td>
        <td><%= business_history.note %></td>
        <td><%= business_history.business_general %></td>
        <td><%= link_to 'Show', business_history %></td>
        <td><%= link_to 'Edit', edit_business_history_path(business_history) %></td>
        <td><%= link_to 'Destroy', business_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business History', new_business_history_path %>
