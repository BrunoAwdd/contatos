<p id="notice"><%= notice %></p>

<h1>Usuario Roles</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuario_roles.each do |usuario_role| %>
      <tr>
        <td><%= usuario_role.name %></td>
        <td><%= link_to 'Show', usuario_role %></td>
        <td><%= link_to 'Edit', edit_usuario_role_path(usuario_role) %></td>
        <td><%= link_to 'Destroy', usuario_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario Role', new_usuario_role_path %>
